<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
	id="WebApp_ID" version="3.1">
	<display-name>Session</display-name>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
	<servlet>
		<servlet-name>SessionValidate</servlet-name>
		<servlet-class>javax.servlet.ServletException</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>Session</servlet-name>
		<servlet-class>Session</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>SessionValidate</servlet-name>
		<url-pattern>/SessionValidate</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>Session</servlet-name>
		<url-pattern>/Session</url-pattern>
	</servlet-mapping>


</web-app>